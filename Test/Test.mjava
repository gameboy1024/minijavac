class Factorial{
    static Void main(String a){
        system.out().println(new Fac.computeFac(10))
    }
}

class Fac {
    Int computeFac(Int num){
        int num_aux
        if (num < 1) {
            num_aux = 1
        } else {
            num_aux = num * (this.computeFac(num-1))
        }
        num_aux
    }
}

